{
  "name": "bestofjs-backend",
  "version": "0.1.0",
  "description": "Best of JavaScript backend applications",
  "scripts": {
    "build": "node cli/tasks/daily-process",
    "daily": "node cli/tasks/daily-process && npm run deploy:prod",
    "deploy:prod": "firebase deploy --project prod",
    "download": "curl https://bestofjs-static-api.now.sh/projects.json > build/projects.json",
    "download:hof": "curl https://bestofjs-static-api.now.sh/hof.json > build/hof.json",
    "daily-npm-update": "node cli/tasks/daily-package-data-process",
    "lint": "eslint **/*.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "serve ./build"
  },
  "author": "Michael Rambeau",
  "license": "MIT",
  "engines": {
    "node": ">=10.14.2"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "emoji-regex": "^8.0.0",
    "fs-extra": "^8.0.1",
    "gh-got": "^8.1.0",
    "got": "^9.5.0",
    "graphql-request": "^1.8.2",
    "immer": "^3.1.3",
    "is-absolute-url": "^3.0.1",
    "lodash": "^4.17.11",
    "luxon": "^1.16.1",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.10",
    "p-map": "^2.0.0",
    "p-props": "^3.0.1",
    "p-timeout": "^3.1.0",
    "package-json": "^6.4.0",
    "pretty-bytes": "^5.2.0",
    "pretty-ms": "^5.0.0",
    "scrape-it": "^5.2.3",
